{
  "name": "@data-ui/build-config",
  "version": "0.0.4",
  "description": "Version-controlled build config for easy re-use and sharing",
  "scripts": {
    "lint": "beemo eslint .",
    "prettier": "beemo prettier \"./{configs,dotfiles}/*.{js,json,md}\"",
    "sync:gitignore": "beemo sync-dotfiles --filter=gitignore"
  },
  "beemo": {
    "module": "@local",
    "drivers": [
      "eslint",
      "prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/williaster/data-ui.git"
  },
  "keywords": [
    "build",
    "config",
    "javascript",
    "beemo"
  ],
  "author": "Chris Williams @williaster",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/williaster/build-tool-config/issues"
  },
  "homepage": "https://github.com/williaster/build-tool-config#readme",
  "dependencies": {
    "@beemo/cli": "^0.19.0",
    "@beemo/core": "^0.19.2",
    "@beemo/driver-babel": "^0.19.1",
    "@beemo/driver-eslint": "^0.19.1",
    "@beemo/driver-jest": "^0.19.1",
    "@beemo/driver-prettier": "^0.19.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-dev": "^2.0.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "canary",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^2.5.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "fs-extra": "^7.0.0",
    "jest": "^23.4.1",
    "jest-runner-eslint": "^0.6.0",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1"
  }
}
